# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
  # creating hash from the given hash_string
  hash_string = ENV["CUSTOME_VARIABLES_STRING"]
  hash_arr = hash_string.split(",")
  hash = {}

  hash_arr.each do |e|
    pair = e.split(":")
    key = pair[0]
    value = pair[1]
    hash[key] = value
  end

end
